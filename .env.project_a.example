# Environment for parallel run: project_a (UI on http://localhost:8080)
# ES config here overrides .env.override so project_a sends telemetry to its own Elastic destination.
ENVIRONMENT=project_a
DEPLOYMENT_ENVIRONMENT=project_a
ENVOY_PORT=8080
ENVOY_ADMIN_PORT=10000
PROMETHEUS_PORT=9090

OTEL_RESOURCE_ATTRIBUTES=service.namespace=opentelemetry-demo,service.version=${IMAGE_VERSION},deployment.environment=project_a
ELASTICSEARCH_ENDPOINT="YOUR_ENDPOINT"
ELASTICSEARCH_API_KEY="YOUR_API_KEY"
