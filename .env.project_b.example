# Environment for parallel run: project_b (UI on http://localhost:8081)
# ES config here overrides .env.override so project_b sends telemetry to its own Elastic destination.
ENVIRONMENT=project_b
DEPLOYMENT_ENVIRONMENT=project_b
ENVOY_PORT=8081
ENVOY_ADMIN_PORT=10001
PROMETHEUS_PORT=9091

OTEL_RESOURCE_ATTRIBUTES=service.namespace=opentelemetry-demo,service.version=${IMAGE_VERSION},deployment.environment=project_b
ELASTICSEARCH_ENDPOINT="YOUR_ENDPOINT"
ELASTICSEARCH_API_KEY="YOUR_API_KEY"
